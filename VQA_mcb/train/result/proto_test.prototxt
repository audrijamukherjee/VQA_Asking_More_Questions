layer {
  name: "data"
  type: "Python"
  top: "data"
  top: "cont"
  top: "img_feature"
  top: "label"
  python_param {
    module: "vqa_data_provider_layer"
    layer: "VQADataProviderLayer"
    param_str: "{\"batchsize\": 24, \"mode\": \"val\"}"
  }
}
layer {
  name: "embed_ba"
  type: "Embed"
  bottom: "data"
  top: "embed_ba"
  embed_param {
    num_output: 300
    input_dim: 15534
    weight_filler {
      type: "uniform"
      min: -0.08
      max: 0.08
    }
  }
}
layer {
  name: "embed"
  type: "TanH"
  bottom: "embed_ba"
  top: "embed"
}
layer {
  name: "lstm1"
  type: "LSTM"
  bottom: "embed"
  bottom: "cont"
  top: "lstm1"
  recurrent_param {
    num_output: 1024
    weight_filler {
      type: "uniform"
      min: -0.08
      max: 0.08
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "slice_first0"
  type: "Slice"
  bottom: "lstm1"
  top: "slice_first0"
  top: "slice_first1"
  top: "slice_first2"
  top: "slice_first3"
  top: "slice_first4"
  top: "slice_first5"
  top: "slice_first6"
  top: "slice_first7"
  top: "slice_first8"
  top: "slice_first9"
  top: "slice_first10"
  top: "slice_first11"
  top: "slice_first12"
  top: "slice_first13"
  top: "lstm1_out"
  slice_param {
    axis: 0
  }
}
layer {
  name: "silence_data_first0"
  type: "Silence"
  bottom: "slice_first0"
}
layer {
  name: "silence_data_first1"
  type: "Silence"
  bottom: "slice_first1"
}
layer {
