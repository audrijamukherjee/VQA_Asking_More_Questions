layer {
  name: "silence_data_first11"
  type: "Silence"
  bottom: "slice_first11"
}
layer {
  name: "silence_data_first12"
  type: "Silence"
  bottom: "slice_first12"
}
layer {
  name: "silence_data_first13"
  type: "Silence"
  bottom: "slice_first13"
}
layer {
  name: "lstm1_reshaped"
  type: "Reshape"
  bottom: "lstm1_out"
  top: "lstm1_reshaped"
  reshape_param {
    shape {
      dim: -1
      dim: 1024
    }
  }
}
layer {
  name: "lstm1_reshaped_droped"
  type: "Dropout"
  bottom: "lstm1_reshaped"
  top: "lstm1_reshaped_droped"
  dropout_param {
    dropout_ratio: 0.3
  }
}
layer {
  name: "lstm1_droped"
  type: "Dropout"
  bottom: "lstm1"
  top: "lstm1_droped"
  dropout_param {
    dropout_ratio: 0.3
  }
}
layer {
  name: "lstm2"
  type: "LSTM"
  bottom: "lstm1_droped"
  bottom: "cont"
  top: "lstm2"
  recurrent_param {
    num_output: 1024
    weight_filler {
      type: "uniform"
      min: -0.08
      max: 0.08
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "slice_second0"
  type: "Slice"
  bottom: "lstm2"
  top: "slice_second0"
  top: "slice_second1"
  top: "slice_second2"
  top: "slice_second3"
  top: "slice_second4"
  top: "slice_second5"
  top: "slice_second6"
  top: "slice_second7"
  top: "slice_second8"
  top: "slice_second9"
  top: "slice_second10"
  top: "slice_second11"
  top: "slice_second12"
  top: "slice_second13"
  top: "lstm2_out"
  slice_param {
    axis: 0
  }
}
layer {
  name: "silence_data_second0"
  type: "Silence"
  bottom: "slice_second0"
}
layer {
  name: "silence_data_second1"
  type: "Silence"
  bottom: "slice_second1"
}
layer {
  name: "silence_data_second2"
  type: "Silence"
  bottom: "slice_second2"
}
